      "cell_type": "code",
      "source": [
        "def bubble(lists):\n",
        "  for i in range(0,len(lists)-1):\n",
        "    for j in range(0,len(lists)-1-i):\n",
        "      if lists[j]>lists[j+1]:\n",
        "        lists[j],lists[j+1]=lists[j+1],lists[j]\n",
        "  return lists"
      ],
